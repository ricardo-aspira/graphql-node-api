// Adicionada lib "esnext" para remover erro de compilação do AsyncIterator e AsyncIterable
{
    "compilerOptions": {
        "lib": [
            "esnext"
        ],
        "target": "es6",
        "module": "commonjs"
    },
    "compileOnSave": false,
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "node_modules/@types/bluebird/index.d.ts"
    ]
}